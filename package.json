{
  "name": "react-nodejs-boilerplate",
  "version": "0.1.0",
  "main": "src/index.tsx",
  "description": "react nodejs boilerplate",
  "author": "Bobrova Alena",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm-run-all --print-label --parallel start:*",
    "----------": "",
    "start:server": "cross-env MODE_ENV=development webpack --progress --color --mode development --watch --config ./webpack/webpack.server.js",
    "start:web": "cross-env MODE_ENV=development webpack --watch --progress --color --mode development --config ./webpack/webpack.client.js",
    "start:mobile": "cross-env MODE_ENV=development IS_MOBILE=mobilemode webpack --watch --progress --color --mode development --config ./webpack/webpack.client.js",
    "build:server": "webpack --color --mode production --config ./webpack/webpack.server.js",
    "build:web": "webpack --color --mode production --config ./webpack/webpack.client.js",
    "build:mobile": "cross-env IS_MOBILE=mobilemode webpack --color --mode production --config ./webpack/webpack.client.js",
    "debug:build:server:dev": "cross-env MODE_ENV=development DEBUG_ENV=debug webpack --progress --color --mode development --config ./webpack/webpack.server.js",
    "debug:build:client:dev": "cross-env MODE_ENV=development DEBUG_ENV=debug webpack --progress --color --mode development --config ./webpack/webpack.client.js",
    "eslint": "eslint --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "eslint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bobrova-alena/website-template.git"
  },
  "dependencies": {
    "@loadable/component": "^5.15.0",
    "@loadable/server": "^5.15.0",
    "@styled-system/css": "^5.1.5",
    "body-parser": "^1.19.0",
    "connected-react-router": "^6.9.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "history": "^5.0.0",
    "mobile-detect": "^1.4.5",
    "mongodb": "^3.6.9",
    "mongodb-client-encryption": "^1.2.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-router-redux": "^4.0.8",
    "redux-saga": "^1.1.3",
    "saslprep": "^1.0.3",
    "styled-components": "^5.3.0",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@loadable/babel-plugin": "^5.13.2",
    "@loadable/webpack-plugin": "^5.15.0",
    "@reduxjs/toolkit": "^1.6.0",
    "@types/express": "^4.17.12",
    "@types/history": "^4.7.9",
    "@types/loadable__component": "^5.13.4",
    "@types/loadable__server": "^5.12.6",
    "@types/loadable__webpack-plugin": "^5.7.3",
    "@types/mongodb": "^3.6.18",
    "@types/node": "^16.3.2",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "@types/react-redux": "^7.1.17",
    "@types/react-router": "^5.1.16",
    "@types/react-router-dom": "^5.1.8",
    "@types/react-router-redux": "^5.0.19",
    "@types/styled-components": "^5.1.11",
    "@types/styled-system": "^5.1.12",
    "@types/webpack-node-externals": "^2.5.2",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "circular-dependency-plugin": "^5.2.2",
    "cross-env": "^7.0.3",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.12",
    "husky": "^5.1.3",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^2.1.0",
    "nodemon-webpack-plugin": "^4.5.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-custom-processor-loader": "^0.6.0",
    "ts-loader": "^9.2.3",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.3.2",
    "typescript-plugin-styled-components": "^1.6.0",
    "webpack": "^5.24.4",
    "webpack-cli": "^4.7.2",
    "webpack-node-externals": "^3.0.0",
    "webpack-stream": "^6.1.2"
  }
}
